<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
          <ExcludeByFile>
            **/Program.cs,
            **/Requests/**/*.cs,
            **/Responses/**/*.cs,
            **/Options/**/*.cs,
            **/Properties/**/*.cs,
            **/Data/**/*.cs,
            **/obj/**/*.cs,
            **/bin/**/*.cs
          </ExcludeByFile>
          <ExcludeByAttribute>
            System.Diagnostics.CodeAnalysis.ExcludeFromCodeCoverageAttribute
          </ExcludeByAttribute>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
